dnl Sets the parties used to run the benchmark.
dnl
changequote([[[[[,]]]]])dnl
dnl
dnl Group over which computation takes place. Size of benchmark. This
dnl influences the size of each individual benchmark and should be
dnl between 1 and 1000.
dnl
define(BENCH_PGROUP, $(vog -gen ECqPGroup -name "P-256"))dnl
define(BENCH_BENCHMARKSIZE, 1)dnl
dnl define(BENCH_BENCHMARKSIZE, 1000)dnl
dnl
dnl define(BENCH_PGROUP, $(vog -gen ECqPGroup -name "P-521"))dnl
dnl define(BENCH_BENCHMARKSIZE, 500)dnl
dnl
dnl define(BENCH_PGROUP, $(vog -gen ModPGroup -fixed 3248))dnl
dnl define(BENCH_BENCHMARKSIZE, 100)dnl
dnl
dnl define(BENCH_PGROUP,)dnl
dnl define(BENCH_BENCHMARKSIZE, 2)dnl
dnl
